{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/the-crew-api/src",
  "monorepo": true,
  "root": "apps/the-crew-api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/the-crew-api/tsconfig.app.json"
  },
  "projects": {
    "the-crew-api": {
      "type": "application",
      "root": "apps/the-crew-api",
      "entryFile": "main",
      "sourceRoot": "apps/the-crew-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/the-crew-api/tsconfig.app.json",
        "plugins": ["@nestjs/swagger"]
      }
    }
  }
}
